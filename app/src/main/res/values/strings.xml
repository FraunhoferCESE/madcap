<resources>
    <!-- the name with that the application is displayed on the device -->
    <string name="app_name">FUNF-Sensor</string>
    <!-- the default pipeline defines which data is uploaded to a defined host -->
     <string name="default_pipeline">
         <!-- a schedule has to be added in here -->
         {"@type":"edu.mit.media.funf.pipeline.BasicPipeline",
         "name":"default",
         "version":1,
         "archive": {
                "@schedule": {"interval": 3000}
         },
         "data":[
         <!-- the data that is uploaded -->
         {"@type": "edu.mit.media.funf.probe.builtin.AccelerometerFeaturesProbe",
                         "@schedule": {"interval": 600, "duration": 10},
                          "goodEnoughAccuracy": 80,
                         "maxWaitTime": 60
                        },
            {"@type": "edu.mit.media.funf.probe.builtin.AccelerometerSensorProbe",
                         "@schedule": {"interval": 600, "duration": 10},
                          "goodEnoughAccuracy": 80,
                         "maxWaitTime": 60
                        },
             {"@type": "edu.mit.media.funf.probe.builtin.BluetoothProbe",
                             "@schedule": {"interval": 600, "duration": 10},
                              "goodEnoughAccuracy": 80,
                             "maxWaitTime": 60
                            },
            {"@type": "edu.mit.media.funf.probe.builtin.CallLogProbe",
                         "@schedule": {"interval": 600, "duration": 10},
                          "goodEnoughAccuracy": 80,
                         "maxWaitTime": 60
                        },
            {"@type": "edu.mit.media.funf.probe.builtin.GravitySensorProbe",
                         "@schedule": {"interval": 600, "duration": 10},
                          "goodEnoughAccuracy": 80,
                         "maxWaitTime": 60
                        },
             {"@type": "edu.mit.media.funf.probe.builtin.GyroscopeSensorProbe",
                             "@schedule": {"interval": 600, "duration": 10},
                              "goodEnoughAccuracy": 80,
                             "maxWaitTime": 60
                            },
            {"@type": "edu.mit.media.funf.probe.builtin.HardwareInfoProbe",
