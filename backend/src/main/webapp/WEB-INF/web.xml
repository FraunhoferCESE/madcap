<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee" version="2.5">
    <filter>
        <filter-name>ObjectifyFilter</filter-name>
        <filter-class>com.googlecode.objectify.ObjectifyFilter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>ObjectifyFilter</filter-name>
        <url-pattern>/goToOfy</url-pattern>
    </filter-mapping>

    <servlet>
        <servlet-name>SystemServiceServlet</servlet-name>
        <!-- That was called servlet-probeType.
             I changed it so it would match the example
             -Frederik-         -->
        <servlet-class>com.google.api.server.spi.SystemServiceServlet</servlet-class>
        <init-param>
            <param-name>services</param-name>
            <!-- That was called param-probeType.
             I changed it so it would match the example
             -Frederik-         -->
            <param-value>
                org.fraunhofer.cese.funf_sensor.backend.apis.ProbeDataSetEndpoint,
                org.fraunhofer.cese.funf_sensor.backend.apis.ResponseDataSetEndpoint
            </param-value>
        </init-param>
    </servlet>
    <servlet-mapping>
        <servlet-name>SystemServiceServlet</servlet-name>


        <url-pattern>/_ah/spi/*</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>myServlet</servlet-name>
        <servlet-class>org.fraunhofer.cese.funf_sensor.backend.servlets.HttpsGetServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>myServlet</servlet-name>
        <url-pattern>/putDataToBucket</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>writeQueue</servlet-name>
        <servlet-class>org.fraunhofer.cese.funf_sensor.backend.servlets.WriteQueue</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>writeQueue</servlet-name>
        <url-pattern>/enqueue</url-pattern>
    </servlet-mapping>


    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>

    <security-constraint>
        <web-resource-collection>
            <web-resource-name>admin</web-resource-name>
            <url-pattern>/download.html</url-pattern>
        </web-resource-collection>
        <auth-constraint>
            <role-name>admin</role-name>
        </auth-constraint>
    </security-constraint>

</web-app>